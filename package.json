    "@aws-amplify/graphql-api-construct": "^1.8.1",
    "aws-sdk": "^2.1462.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongodb": "^5.1.0",
    "mongoose": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "socket.io": "^4.6.1",
    "svgo": "^3.2.0"
  }
}
